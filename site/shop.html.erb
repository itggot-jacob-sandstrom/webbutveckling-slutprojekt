<% 2.times do |i| %>

    <article class="shopbox">
        <h3>Image pack <%= i+1 %></h3>
        <p><%= i*4+69 %> $</p>
        <div class="shopimgboxbox">
            <% 6.times do |j| %>
                <div class="shopimgbox" onclick="popup(<%= i*6 + j %>)" ></div>
            <% end %>
        </div>
    </article>

<% end %>

